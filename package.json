{
  "name": "vue-windi",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-windi.umd.js",
  "module": "./dist/vue-windi.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-windi.es.js",
      "require": "./dist/vue-windi.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.34.9",
    "windicss": "^3.5.1",
    "windicss-analysis": "^0.3.5"
  }
}
